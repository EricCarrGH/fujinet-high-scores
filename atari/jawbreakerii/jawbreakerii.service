[Unit]
Description=Atari Frogger Hi-scores
After=remote-fs.target
After=syslog.target

# replace /tnfs with your TNFS directory

[Service]
User=root
Group=root
ExecStart=/usr/local/sbin/jawbreakerii "/tnfs/Atari 8-bit/Games/High Score Enabled/Jawbreaker II.atr" "/var/www/atari-scores/jawbreaker-ii.html"

[Install]
WantedBy=multi-user.target
